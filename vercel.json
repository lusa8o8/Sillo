{
    "version": 2,
    "buildCommand": "npm run build",
    "functions": {
        "api/index.ts": {
            "includeFiles": "dist/public/**"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api"
        },
        {
            "src": "/(.*)",
            "dest": "/api"
        }
    ]
}